{
    "foam.openDailyNote.directory": "./src/garden/daily-notes/posts",
    "foam.files.newNotePath": "currentDir",
    "deno.enable": true,
    "gitlens.remotes": [
        {
            "domain": "mau.dev",
            "type": "GitLab"
        }
    ],
    "conventionalCommits.scopes": [
        "api",
        "global",
        "mkdocs",
        "pipenv",
        "stylesheets",
        "announcement",
        "daily-notes",
        "changelog",
        "multiverse",
        "legal",
        "gitlab-ci",
        "readme",
        "issue-templates",
        "github"
    ],
    "conventionalCommits.autoCommit": false,
    "git.alwaysSignOff": true,
    "python.analysis.autoImportCompletions": true,
    "json.schemaDownload.enable": true,
    "json.schemas": [
        {
            "fileMatch": [
                ".denoflare*"
            ],
            "schema": true,
            "url": "https://raw.githubusercontent.com/skymethod/denoflare/v0.5.12/common/config.schema.json"
        }
    ],
    "yaml.schemas": {
        "https://squidfunk.github.io/mkdocs-material/schema.json": "mkdocs.yml",
        "https://json.schemastore.org/github-issue-config.json": ".github/ISSUE_TEMPLATE/config.yml"
    },
    "yaml.customTags": [ 
        "!ENV scalar",
        "!ENV sequence",
        "!relative scalar",
        "tag:yaml.org,2002:python/name:material.extensions.emoji.to_svg",
        "tag:yaml.org,2002:python/name:material.extensions.emoji.twemoji",
        "tag:yaml.org,2002:python/name:pymdownx.superfences.fence_code_format"
    ],
    "emojisense.unicodeCompletionsEnabled": false,
    "emojisense.markupCompletionsEnabled": true,
    "emojisense.emojiDecoratorsEnabled": true
}
